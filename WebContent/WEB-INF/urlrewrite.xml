<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<!--

    Configuration file for UrlRewriteFilter
    http://www.tuckey.org/urlrewrite/

-->
<urlrewrite decode-using="null">

	<rule>
		<from>^/index$</from>
		<to type="forward">/index.jsp</to>
	</rule>
	<rule>
		<from>^/login$</from>
		<to type="forward">/login.jsp</to>
	</rule>
	<rule>
		<from>^/logout$</from>
		<to type="forward">/logout.jsp</to>
	</rule>
	<rule>
		<from>^/error$</from>
		<to type="forward">/error.jsp</to>
	</rule>


	<rule>
		<from>^/userCreate$</from>
		<to type="forward">/userCreate.jsp</to>
	</rule>
	<rule>
		<from>^/userEdit$</from>
		<to type="forward">/userEdit.jsp</to>
	</rule>
	<rule>
		<from>^/userList$</from>
		<to type="forward">/userList.jsp</to>
	</rule>
   

</urlrewrite>

